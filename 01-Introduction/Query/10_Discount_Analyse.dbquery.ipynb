   "outputs": [],
   "source": [
    "SELECT p.Name, SUM(od.UnitPriceDiscount * od.OrderQty) AS TotalDiscountAmount\n",
    "FROM SalesLT.SalesOrderDetail od\n",
    "JOIN SalesLT.Product p ON od.ProductID = p.ProductID\n",
    "GROUP BY p.ProductID, p.Name\n",
    "ORDER BY TotalDiscountAmount DESC\n",
    "LIMIT 1;"
   ]
  }
