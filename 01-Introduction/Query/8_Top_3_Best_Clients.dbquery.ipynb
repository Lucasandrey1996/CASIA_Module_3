   "source": [
    "SELECT c.FirstName, c.LastName, SUM(oh.TotalDue) AS TotalRevenue\n",
    "FROM bronze.Customer c\n",
    "JOIN bronze.SalesOrderHeader oh ON c.CustomerID = oh.CustomerID\n",
    "GROUP BY c.CustomerID, c.FirstName, c.LastName\n",
    "ORDER BY TotalRevenue DESC\n",
    "LIMIT 3;"
